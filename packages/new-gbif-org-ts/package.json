{
  "name": "new-gbif-org-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "npm run build:client && npm run build:server && npm run build:hp",
    "develop": "NODE_ENV=development node server.js",
    "build:client": "vite build --outDir dist/client --ssrManifest --config vite.gbif.config.js",
    "build:server": "vite build --ssr src/entry.server.jsx --outDir dist/server --config vite.gbif.config.js",
    "start": "NODE_ENV=production node server.js",
    "build:hp": "vite build --outDir dist/lib --config vite.hp.config.js",
    "start:hp": "node server.hp.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "ol": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-router-dom": "^6.18.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}