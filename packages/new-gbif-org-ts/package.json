{
  "name": "new-gbif-org-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "develop": "NODE_ENV=development node gbif/server.js",
    "build:client": "vite build --config gbif/vite.config.ts --outDir ../dist/gbif/client --ssrManifest",
    "build:server": "vite build --config gbif/vite.config.ts --outDir ../dist/gbif/server --ssr ../src/gbif/entry.server.tsx",
    "build": "npm run build:client && npm run build:server",
    "start": "NODE_ENV=production node gbif/server.js",

    "build:hp": "vite build --config hp/vite.config.ts --outDir ../dist/hp",
    "start:hp": "node hp/server.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "ol": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-router-dom": "^6.18.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}